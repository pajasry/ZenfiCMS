{
    "name": "zenficms-client",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "prod": "vite preview",
        "check:types": "tsc",
        "lint": "npx eslint src",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "npx prettier src --check",
        "prettier:fix": "npm run prettier -- --write",
        "graphql:documents": "gqlg --schemaFilePath ../schema.gql --destDirPath src/graphql/documents --depthLimit 5",
        "graphql:generate": "graphql-codegen -r dotenv/config",
        "graphql": "concurrently npm:graphql:*"
    },
    "dependencies": {
        "@apollo/client": "^3.6.9",
        "@craftjs/core": "^0.2.0-beta.6",
        "@reduxjs/toolkit": "^1.8.2",
        "axios": "^0.27.2",
        "dayjs": "^1.11.3",
        "graphql": "^16.5.0",
        "lodash": "^4.17.21",
        "polished": "^4.2.2",
        "querystring": "^0.2.1",
        "react": "^18.0.0",
        "react-contenteditable": "^3.3.6",
        "react-dom": "^18.0.0",
        "react-helmet": "^6.1.0",
        "react-query": "^3.39.1",
        "react-redux": "^8.0.2",
        "react-router-dom": "^6.3.0",
        "react-select": "^5.3.2",
        "styled-components": "^5.3.5"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "2.10.0",
        "@graphql-codegen/introspection": "2.2.0",
        "@graphql-codegen/typescript": "2.7.2",
        "@graphql-codegen/typescript-operations": "2.5.2",
        "@types/axios": "^0.14.0",
        "@types/lodash": "^4.14.182",
        "@types/node": "^18.6.1",
        "@types/react": "^18.0.0",
        "@types/react-datepicker": "^4.4.2",
        "@types/react-dom": "^18.0.0",
        "@types/react-helmet": "^6.1.5",
        "@types/styled-components": "^5.1.25",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "@vitejs/plugin-react": "^1.3.0",
        "babel-plugin-styled-components": "^2.0.7",
        "concurrently": "^7.3.0",
        "dotenv": "^16.0.1",
        "eslint": "^8.17.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.5.0",
        "gql-generator": "^1.0.17",
        "prettier": "2.6.2",
        "sass": "^1.52.2",
        "typescript": "^4.6.3",
        "vite": "^2.9.9",
        "vite-tsconfig-paths": "^3.5.0"
    }
}
